<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>RuiChen.Abp.Account.Web.OAuth</AssemblyName>
    <PackageId>RuiChen.Abp.Account.Web.OAuth</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>RuiChen.Abp.Account.Web.OAuth</RootNamespace>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="wwwroot\**\*.js" />
    <EmbeddedResource Include="wwwroot\**\*.css" />
    <EmbeddedResource Include="wwwroot\**\*.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AspNet.Security.OAuth.Bilibili" />
    <PackageReference Include="AspNet.Security.OAuth.GitHub" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RuiChen.Abp.Account.OAuth\RuiChen.Abp.Account.OAuth.csproj" />
    <ProjectReference Include="..\RuiChen.Abp.Account.Web\RuiChen.Abp.Account.Web.csproj" />
  </ItemGroup>

</Project>
